{
  "name": "bingo",
  "version": "0.0.1",
  "description": "Bingo app",
  "scripts": {
    "coverage:serve": "node dev/http-server.js coverage",
    "docs": "jsdoc -c config/jsdoc.conf.json",
    "docs:serve": "node dev/http-server.js docs",
    "lint": "eslint src tests",
    "start": "concurrently --kill-others 'npm run test:continuous' 'npm run coverage:serve' 'npm run webpack' 'npm run watch' 'npm run docs:serve'",
    "test": "karma start config/karma.conf.js --singleRun",
    "test:continuous": "karma start config/karma.conf.js",
    "watch": "watch 'npm run lint & npm run docs' src",
    "webpack": "webpack-dev-server --config config/webpack.config.dev.js --devtool source-map --progress --colors --inline --hot --content-base dist/"
  },
  "devDependencies": {
    "angular": "^1.5.8",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "browser-sync": "^2.17.0",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "eslint": "^3.7.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "jsdoc": "^3.4.2",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-notify-reporter": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "lodash": "^4.16.4",
    "mocha": "^3.1.2",
    "node-sass": "^3.10.1",
    "normalize.css": "^5.0.0",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^1.0.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
